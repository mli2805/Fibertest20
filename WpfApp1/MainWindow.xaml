<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <TextBox x:Name="T1" Margin="5" Text="30"/>
        <TextBox x:Name="T2" Margin="5" Text="30"/>
        <TextBox x:Name="T3" Margin="5" Text="40"/>
        <TextBox x:Name="T4" Margin="5" Text="40"/>
        <Button Width="120" Margin="5" Height="24" Content="Start" Click="Button_Click" />
        <TextBlock x:Name="DOld" Margin="5"/>
        <TextBlock x:Name="DNew" Margin="5"/>
        <TextBlock x:Name="Middle" Margin="5"/>
    </StackPanel>
</Window>
