<UserControl x:Class="Iit.Fibertest.TestBench.GpsInputControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:testBench="clr-namespace:Iit.Fibertest.TestBench"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel>
        <ComboBox Width="150"/>
        <testBench:OneGpsCoorInputControl x:Name="Lat" Margin="5" Value="53.5" LatOrLng="Latitude" InputMode="Degrees"/>
        <testBench:OneGpsCoorInputControl x:Name="Lng" Margin="5" Value="28.5" LatOrLng="Longitude" InputMode="Degrees"/>
        <Button Content="Cancel"/>
    </StackPanel>
</UserControl>
