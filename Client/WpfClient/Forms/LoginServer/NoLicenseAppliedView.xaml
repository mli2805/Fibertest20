<Window x:Class="Iit.Fibertest.Client.NoLicenseAppliedView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Height="250" Width="500">
    <StackPanel>
        <!-- <TextBlock Text="No license has been applied yet!" Margin="5,60, 5, 5" HorizontalAlignment="Center" FontWeight="Bold"/> -->
        <TextBlock Text="Лицензия еще не была применена!" Margin="5,40, 5, 5" HorizontalAlignment="Center" FontWeight="Bold"/>
        <!-- <TextBlock Text="You can apply DEMO license or load your license from file" Margin="5" HorizontalAlignment="Center"/> -->
        <TextBlock Text="Вы можете применить ДЕМО лицензию или загрузить Вашу лицензию из файла." Margin="5,20,5,5" HorizontalAlignment="Center"/>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="0,50,0, 20">
            <Button Name="ApplyDemoLicense" Content="Apply Demo License" Margin="10" Width="200"/>
            <Button Name="LoadLicenseFromFile" Content="Load License From File" Margin="10" Width="200"/>
        </StackPanel>
    </StackPanel>
</Window>
