        <Button Content="Close" DockPanel.Dock="Bottom" Margin="5" Width="120" Name="Close"></Button>
        <StackPanel Margin="5" Orientation="Horizontal" DockPanel.Dock="Top">
            <Button Content="Select folder" Margin="5" Width="120"/>
            <TextBlock/>
        </StackPanel>
        <StackPanel>

        </StackPanel>
        
    </DockPanel>
</Window>